    "browserify": "^12.0.1",
    "browserify-shim": "^3.8.11",
    "del": "^2.1.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-jshint": "^1.12.0",
    "gulp-less": "^3.0.5",
    "gulp-minify-css": "^1.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "jshint-stylish": "^2.1.0",
    "run-sequence": "^1.1.5",
    "vinyl-buffer": "^1.0.0",
